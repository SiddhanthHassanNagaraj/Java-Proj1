pipeline {
    agent { label 'MAS' }
    triggers {
        pollSCM 'H/3 * * * *'
    }
    stages {
        stage ("SCM Chkout-1"){
            steps {
                git 'https://github.com/Vishwanathms/Java-Proj1'
            }
        }
        stage ("Java execute") {
            steps {
                sh "javac *.java"
                sh "java Simple"
            }
        }
    }
}
